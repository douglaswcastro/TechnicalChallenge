@model IEnumerable<TechnicalChallenge.Models.Numeros>

<h1>Divisores de: @ViewBag.Numero</h1>

<table class="table">
   
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Numero)
                </td>

            </tr>
        }
    </tbody>
</table>


<h1>Divisores Primos de: @ViewBag.Numero</h1>

<table class="table">
    
    <tbody>
        @foreach (var item in Model.Where(m=> m.Primo))
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Numero)
                </td>

            </tr>
        }
    </tbody>
</table>
